{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Houston",
  "author": "Derived from VS Code Theme",
  "themes": [
    {
      "appearance": "dark",
      "name": "Houston",
      "style": {
        "background": "#17191e",
        "editor.background": "#17191e",
        "editor.foreground": "#eef0f9",
        "editor.gutter.background": "#17191e",
        "editor.line_number": "#858b98",
        "editor.active_line_number": "#eef0f9",
        "editor.active_line.background": "#23262d",
        "editor.highlighted_line.background": "#23262d",
        "editor.indent_guide": "#858b981e",
        "editor.indent_guide_active": "#858b9866",
        "editor.cursor": "#eef0f9",
        "editor.selection": "#eef0f933",
        "editor.active_selection": "#343841",
        "editor.selection_border": "#858b98",
        "text": "#eef0f9",
        "text.muted": "#858b98",
        "text.placeholder": "#858b98",
        "surface.background": "#17191e",
        "panel.background": "#23262d",
        "title_bar.background": "#23262d",
        "title_bar.inactive_background": "#23262d",
        "status_bar.background": "#23262d",
        "toolbar.background": "#17191e",
        "tab_bar.background": "#23262d",
        "tab.active_background": "#17191e",
        "tab.inactive_background": "#23262d",
        "tab.active_bottom_border": "#eef0f9",
        "element.background": "#23262d",
        "element.hover": "#343841",
        "element.selected": "#343841",
        "border": "#23262d",
        "border.focused": "#23262d",
        "pane.focused_border": "#23262d",
        "panel.focused_border": "#23262d",
        "error": "#dc3657",
        "warning": "#ffd493",
        "info": "#00daef",
        "scrollbar.thumb.background": "#54586466",
        "scrollbar.thumb.hover_background": "#54586499",
        "terminal.foreground": "#cccccc",
        "terminal.ansi.black": "#17191e",
        "terminal.ansi.red": "#dc3657",
        "terminal.ansi.green": "#23d18b",
        "terminal.ansi.yellow": "#ffc368",
        "terminal.ansi.blue": "#2b7eca",
        "terminal.ansi.magenta": "#ad5dca",
        "terminal.ansi.cyan": "#24c0cf",
        "terminal.ansi.white": "#eef0f9",
        "terminal.ansi.bright_black": "#545864",
        "terminal.ansi.bright_red": "#f4587e",
        "terminal.ansi.bright_green": "#4bf3c8",
        "terminal.ansi.bright_yellow": "#ffd493",
        "terminal.ansi.bright_blue": "#54b9ff",
        "terminal.ansi.bright_magenta": "#cc75f4",
        "terminal.ansi.bright_cyan": "#00daef",
        "terminal.ansi.bright_white": "#fafafa",
        "syntax": {
          "comment": {
            "color": "#eef0f98f",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#54b9ff"
          },
          "operator": {
            "color": "#eef0f9"
          },
          "function": {
            "color": "#00daef"
          },
          "tag": {
            "color": "#54b9ff"
          },
          "attribute": {
            "color": "#acafff"
          },
          "string": {
            "color": "#ffd493"
          },
          "number": {
            "color": "#ffd493"
          },
          "boolean": {
            "color": "#ffd493"
          },
          "constant": {
            "color": "#ffd493"
          },
          "type": {
            "color": "#acafff"
          },
          "constructor": {
            "color": "#acafff"
          },
          "variable": {
            "color": "#4bf3c8"
          },
          "variable.special": {
            "color": "#acafff"
          },
          "property": {
            "color": "#4bf3c8"
          },
          "punctuation": {
            "color": "#eef0f9"
          }
        }
      }
    }
  ]
}
